###  https://www.petwin.org/o3po-resources/o3po#component_of
:component_of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000050> ;
              owl:inverseOf :has_component ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000040> ;
              rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> ;
              <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. a part of relation that holds betweentwo material entities"@en ;
              rdfs:label "component of"@en ,
                         "componente de"@pt .


###  https://www.petwin.org/o3po-resources/o3po#connected_to
:connected_to rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000040> ;
              rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> ;
              rdfs:label "conectado a"@pt ,
                         "connected to"@en .


###  https://www.petwin.org/o3po-resources/o3po#exchanges_fluid_with
:exchanges_fluid_with rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :connected_to ;
                      rdf:type owl:SymmetricProperty ;
                      rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000040> ;
                      rdfs:range <http://purl.obolibrary.org/obo/BFO_0000040> ;
                      rdfs:comment "Relation between two things which have a fluid exchange connection"@en ;
                      rdfs:label "exchanges fluid with"@en ,
                                 "troca fluido com"@pt ;
                      <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :exchanges_fluid_with ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation between two things which have a fluid exchange connection"@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#feeds_fluid_to
:feeds_fluid_to rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :exchanges_fluid_with ;
                owl:inverseOf :has_fluid_fed_by ;
                rdfs:comment "Relation from a thing feeding fluid to the thing it is feeding fluid to."@en ;
                rdfs:label "alimenta fluido para"@pt ,
                           "feeds fluid to"@en ;
                <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :feeds_fluid_to ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation from a thing feeding fluid to the thing it is feeding fluid to."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#has_component
:has_component rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000051> ;
               rdf:type owl:TransitiveProperty ;
               rdfs:label "has component"@en ,
                          "tem component"@pt .


###  https://www.petwin.org/o3po-resources/o3po#has_fluid_fed_by
:has_fluid_fed_by rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :exchanges_fluid_with ;
                  rdfs:comment "Relation from a thing that is fed fluid to the thing feeding the fluid."@en ;
                  rdfs:label "has fluid fed by"@en ,
                             "tem fluido alimentado por"@pt ;
                  <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :has_fluid_fed_by ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation from a thing that is fed fluid to the thing feeding the fluid."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#has_fluid_returned_by
:has_fluid_returned_by rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :feeds_fluid_to ;
                       owl:inverseOf :returns_fluid_to ;
                       rdfs:comment "Relation between a thing that has fluid returned to it and the thing returning the fluid."@en ;
                       rdfs:label "has fluid returned by"@en ,
                                  "tem fluido retornado por"@pt ;
                       <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :has_fluid_returned_by ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation between a thing that has fluid returned to it and the thing returning the fluid."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#has_fluid_supplied_by
:has_fluid_supplied_by rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :feeds_fluid_to ;
                       owl:inverseOf :supplies_fluid_to ;
                       rdfs:comment "Relation between a thing that has fluid supplied to it and the thing supplying the fluid."@en ;
                       rdfs:label "has fluid supplied by"@en ,
                                  "tem fluido fornecido por"@pt ;
                       <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :has_fluid_supplied_by ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation between a thing that has fluid supplied to it and the thing supplying the fluid."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#injects
:injects rdf:type owl:ObjectProperty ;
         <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. to introduce (something) into a passage, cavity, or solid material under pressure."@en ;
         rdfs:label "injects"@en ;
         rdfs:seeAlso "https://www.lexico.com/en/definition/inject"@en .


###  https://www.petwin.org/o3po-resources/o3po#measures
:measures rdf:type owl:ObjectProperty .


###  https://www.petwin.org/o3po-resources/o3po#produces
:produces rdf:type owl:ObjectProperty ;
          rdfs:label "produces"@en ;
          rdfs:seeAlso "def. to make or manufacture from components or raw materials."@en ,
                       "https://www.lexico.com/en/definition/produce"@en .


###  https://www.petwin.org/o3po-resources/o3po#returns_fluid_to
:returns_fluid_to rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :has_fluid_fed_by ;
                  rdfs:comment "Relation implying logical return flow from a thing to another."@en ;
                  rdfs:label "retorna fluido para"@pt ,
                             "returns fluid to"@en ;
                  <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :returns_fluid_to ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation implying logical return flow from a thing to another."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#separates
:separates rdf:type owl:ObjectProperty ;
           <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. to cause to move or be apart."@en ;
           rdfs:label "separates"@en ;
           rdfs:seeAlso "https://www.lexico.com/en/definition/separate"@en .


###  https://www.petwin.org/o3po-resources/o3po#stores
:stores rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:AsymmetricProperty ,
                 owl:IrreflexiveProperty ;
        <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. to keep or accumulate (something) for future use."@en ;
        rdfs:label "stores"@en ;
        rdfs:seeAlso "https://www.lexico.com/en/definition/store"@en .


###  https://www.petwin.org/o3po-resources/o3po#supplies_fluid_to
:supplies_fluid_to rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :has_fluid_fed_by ;
                   rdfs:comment "Relation implying logical supply flow from a thing to another."@en ;
                   rdfs:label "fornece fluido para"@pt ,
                              "supplies fluid to"@en ;
                   <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "http://www.w3id.org/fso"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :supplies_fluid_to ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Relation implying logical supply flow from a thing to another."@en ;
   <http://purl.org/dc/terms/source> "Flow Systems Ontology (FSO)"@en
 ] .


###  https://www.petwin.org/o3po-resources/o3po#transports
:transports rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. the action of transporting something or the state of being transported"@en ;
            rdfs:label "transports"@en ;
            rdfs:seeAlso "https://www.lexico.com/en/definition/transport"@en .


###  https://www.petwin.org/o3po-resources/o3po#treats
:treats rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ,
                 owl:AsymmetricProperty ,
                 owl:IrreflexiveProperty ;
        <http://www.ontologyrepository.com/CommonCoreOntologies/definition> "def. to apply a process or a substance to (something) to protect or preserve it or to give it particular properties."@en ;
        rdfs:label "treats"@en ;
        rdfs:seeAlso "https://www.lexico.com/en/definition/treat"@en .


